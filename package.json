{
  "name": "postman-to-file",
  "version": "1.0.1",
  "description": "Convert Postman collections to AI-friendly file structures and back",
  "main": "dist/cli.js",
  "bin": {
    "postman-to-file": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "prepublishOnly": "npm run build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:integration": "./test-scripts/test-all.sh"
  },
  "keywords": [
    "postman",
    "api",
    "converter",
    "cli",
    "ai",
    "file-structure",
    "collection"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/node": "^24.9.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
